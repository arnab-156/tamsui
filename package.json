{
  "name": "exercise-js",
  "version": "0.0.0",
  "description": "Exercises for JavaScript and Node",
  "main": "index.js",
  "scripts": {
    "analyze": "webpack-bundle-analyzer stats.json",
    "build:dev": "npm run clean && webpack --config webpack-config.js --profile --json > stats.json",
    "build:dev:analyze": "npm run clean && npm run build:dev && npm run analyze",
    "build:dev:run": "npm run build:dev && node dist/index.js | npx pino-pretty -c -S",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .js,.jsx,.mjs,.ts,.tsx .",
    "start": "echo 'TODO: build/build+run production assets'",
    "watch": "webpack --config webpack-config.js --watch | npx pino-pretty -c -S",
    "test": "jest",
    "test:snapshot": "jest --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chichiwang/exercise-js.git"
  },
  "keywords": [
    "javaScript",
    "react",
    "developer",
    "katas"
  ],
  "author": "Chi-chi Wang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chichiwang/exercise-js/issues"
  },
  "homepage": "https://github.com/chichiwang/exercise-js#readme",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-jest": "^29.7.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon-webpack-plugin": "^4.8.2",
    "pino-pretty": "^11.0.0",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "express": "^4.19.2",
    "pino-http": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  }
}
